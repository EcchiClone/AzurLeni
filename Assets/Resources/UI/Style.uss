    background-color: rgba(33, 32, 49, 0.88);
    border-left-color: rgb(90, 154, 255);
    border-right-color: rgb(90, 154, 255);
    border-top-color: rgb(90, 154, 255);
    border-bottom-color: rgb(90, 154, 255);
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    align-items: center;
    align-self: center;
    justify-content: center;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    position: absolute;
    margin-top: 0;
