.button-lobby-bottom-menu {
    flex-grow: 0;
    width: 192px;
    height: 55px;
    background-color: rgb(255, 255, 255);
    border-top-left-radius: 5px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 10px;
    justify-content: center;
    font-size: 44px;
    color: rgb(41, 44, 49);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    transition-property: scale;
    transition-duration: 0.1s;
    border-top-color: rgb(157, 157, 166);
    border-right-color: rgb(139, 139, 149);
    border-bottom-color: rgb(163, 163, 164);
    border-left-color: rgb(130, 131, 137);
    -unity-text-align: middle-center;
    align-items: flex-start;
    align-self: auto;
    padding-left: 40px;
}

.button-lobby-bottom-menu:hover {
    scale: 1.05 1.05;
}

.button-lobby-bottom-menu:active {
    background-color: rgb(206, 203, 206);
    scale: 1 1;
}

.character {
    background-image: url('project://database/Assets/Images/CRT.png?fileID=21300000&guid=765a2af6bd6ab0844ba5c197d7d0f395&type=3#CRT');
    background-color: rgba(0, 0, 0, 0);
    width: 100%;
    height: 100%;
    transition-duration: 0.5s;
}

.tab {
    position: absolute;
    width: 100%;
    height: 100%;
    transition-duration: 0.3s, 0.3s;
    transition-timing-function: ease-in-out-cubic, ease-in-out-cubic;
    opacity: 1;
    transition-property: translate, opacity;
}

.tab--left {
    translate: -100% 0;
    opacity: 0;
    transition-property: translate, opacity;
    transition-duration: 0.3s, 0.3s;
    transition-timing-function: ease-in-out-cubic, ease-in-out-cubic;
}

.tab--right {
    translate: 100% 0;
    opacity: 0;
    transition-property: translate, opacity;
    transition-duration: 0.3s, 0.3s;
    transition-timing-function: ease-in-out-cubic, ease-in-out-cubic;
}

.ui-azur-rhombus-1 {
    rotate: 45deg;
